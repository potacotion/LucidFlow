#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/workplace/temp/WEB/LucidFlow/backend/node_modules/vite/bin/node_modules:/mnt/d/workplace/temp/WEB/LucidFlow/backend/node_modules/vite/node_modules:/mnt/d/workplace/temp/WEB/LucidFlow/backend/node_modules:/mnt/d/workplace/temp/WEB/LucidFlow/node_modules:/mnt/d/workplace/temp/WEB/node_modules:/mnt/d/workplace/temp/node_modules:/mnt/d/workplace/node_modules:/mnt/d/node_modules:/mnt/d/workplace/temp/WEB/LucidFlow/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/workplace/temp/WEB/LucidFlow/backend/node_modules/vite/bin/node_modules:/mnt/d/workplace/temp/WEB/LucidFlow/backend/node_modules/vite/node_modules:/mnt/d/workplace/temp/WEB/LucidFlow/backend/node_modules:/mnt/d/workplace/temp/WEB/LucidFlow/node_modules:/mnt/d/workplace/temp/WEB/node_modules:/mnt/d/workplace/temp/node_modules:/mnt/d/workplace/node_modules:/mnt/d/node_modules:/mnt/d/workplace/temp/WEB/LucidFlow/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
